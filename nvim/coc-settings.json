{
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.preferences.formatOnType": true,
  "coc.preferences.willSaveHandlerTimeout": 1000,
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "python",
    "markdown",
    "javascript",
    "typescript",
    "javascript.vue",
    "javascript.jsx",
    "json",
    "jsonc",
    "scss",
    "less",
    "css"
  ],
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "tsserver.enableJavascript": false,
  "languageserver": {
    "graphql": {
      "command": "graphql-lsp",
      "args": ["server", "-m", "stream"],
      "filetypes": [
        "typescript",
        "typescriptreact",
        "graphql",
        "javascript",
        "javascriptreact"
      ]
    },
    "puppet": {
      "command": "ruby",
      "args": ["/Users/ioannis.karasavvaidis/.config/nvim/LS/puppet-editor-services/puppet-languageserver","--stdio","--puppet-settings=--moduledir,/path/to/module/path"],
      "cwd": "/Users/ioannis.karasavvaidis/.config/nvim/LS/puppet-editor-services",
      "filetypes": ["puppet"],
      "rootPatterns": [".git/", "metadata.json"]
    }
  }
}
